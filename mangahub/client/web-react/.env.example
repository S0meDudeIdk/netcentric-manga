# Backend API URL (if not localhost)
# REACT_APP_BACKEND_URL=http://your-backend-url:8080

# Enable gRPC globally for all features
# REACT_APP_USE_GRPC=true

# Or enable gRPC for specific features:
# Enable gRPC for library operations (getLibrary, addToLibrary, etc.)
REACT_APP_USE_GRPC_LIBRARY=true

# Enable gRPC for progress updates (with TCP broadcast)
REACT_APP_USE_GRPC_PROGRESS=true

# Enable gRPC for rating system
REACT_APP_USE_GRPC_RATING=true

# Enable gRPC for local manga search
# REACT_APP_USE_GRPC_SEARCH=false
